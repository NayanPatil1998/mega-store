\n    const override = css`\n    margin: 50% 50%;\n      top: 50%;\n    border-color: red;\n  `;\n\n    function addCart(){\n        dispatch(addToCart(product));\n        toast.success(\"Added to cart successfully\")\n    }\n\n    React.useEffect(() => {\n\n        axios.get<IProduct>(`${baseUrl}/product/${id}`)\n            .then((res: AxiosResponse<IProduct>) => {\n                setProduct(res.data);\n                setLoading(false);\n            })\n            .catch(err => {\n                console.log(err)\n                setError(err);\n            })\n    }, [id])\n    const setLoad= () =>{\n        setLoading(false)\n    }\n    return(\n        <div>\n            <Header />\n        <div className=\"ProductDetails\">\n            {\n                loading ? (\n                    <HashLoader loading={loading} css={override} size={150} />\n                ) : (\n                    <div className=\"container mb-2\" id=\"product-section\">\n                        <div className=\"row\">\n                            <div className=\"col-md-5 mb-4\">\n                                <img onLoad={setLoad} src={product.image} className=\"img-fluid productImage\" alt=\"...\" />\n\n                            </div>\n                            <div className=\"col-md-6\">\n                                <div className=\"col-md-12\">\n                                    <p className=\"h2 mb-3\">{product.title}</p>\n                                    <h5><span className=\"badge bg-secondary mb-3\">{product.category}</span></h5>\n                                    <div className=\"col-md-3 mb-3\">\n                                        <Star />\n                                        <Star />\n                                        <Star />\n                                        <Star />\n                                        <StarBorder />\n                                    </div>\n                                    <p className=\"h3 mb-3\">₹ 